<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Must_depart_after_you_arrive</fullName>
    <active>true</active>
    <errorConditionFormula>Arrive__c &gt;  Depart__c</errorConditionFormula>
    <errorDisplayField>Arrive__c</errorDisplayField>
    <errorMessage>You cannot depart before you arrive.</errorMessage>
</ValidationRule>
