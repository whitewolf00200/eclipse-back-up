<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>In_Vehicle</fullName>
    <active>true</active>
    <errorConditionFormula>AND(NOT(ISPICKVAL( Transaction_Type__c , &quot;In Vehicle&quot;) )  ,Vehicle__c  &lt;&gt; &apos;&apos;)</errorConditionFormula>
    <errorDisplayField>Transaction_Type__c</errorDisplayField>
    <errorMessage>Status should be In Vehicle if Vehicle is filled.</errorMessage>
</ValidationRule>
