<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Serial_Number_length</fullName>
    <active>false</active>
    <errorConditionFormula>LEN( Name )  &gt; 80</errorConditionFormula>
    <errorDisplayField>Name</errorDisplayField>
    <errorMessage>Serial Number exceeds the maximum length</errorMessage>
</ValidationRule>
