<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Time_Allowed_for_Visit_Validatation</fullName>
    <active>true</active>
    <description>End Date is required for Time Allowed for Visit Validatation</description>
    <errorConditionFormula>AND(ISBLANK(End_Date__c ), NOT( ISBLANK( Time_Allowed_for_Visit__c ) ))</errorConditionFormula>
    <errorDisplayField>End_Date__c</errorDisplayField>
    <errorMessage>End Date is required for Time Allowed for Visit</errorMessage>
</ValidationRule>
