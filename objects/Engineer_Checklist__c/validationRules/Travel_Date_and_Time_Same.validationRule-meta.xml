<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Travel_Date_and_Time_Same</fullName>
    <active>true</active>
    <errorConditionFormula>Start_Travel_Time__c   &gt;=   End_Travel_Time__c</errorConditionFormula>
    <errorMessage>End Date Time should not be lesser than or equal to Start Travel Time</errorMessage>
</ValidationRule>
