<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Travel_Time_not_same</fullName>
    <active>false</active>
    <errorConditionFormula>Depart__c&gt;=Arrive__c</errorConditionFormula>
    <errorDisplayField>Depart__c</errorDisplayField>
    <errorMessage>The Departure time must be lesser than Arrival time</errorMessage>
</ValidationRule>
