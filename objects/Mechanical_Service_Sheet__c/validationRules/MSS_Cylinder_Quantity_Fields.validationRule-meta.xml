<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>MSS_Cylinder_Quantity_Fields</fullName>
    <active>true</active>
    <errorConditionFormula>AND(NOT(ISBLANK(Cylinders_Qty__c)), 
OR(ISBLANK(Cylinders_Pass__c )), ISBLANK(Cylinders_Fail__c ))</errorConditionFormula>
    <errorMessage>Please enter Pass or Fail for Cylinder Quantity</errorMessage>
</ValidationRule>
